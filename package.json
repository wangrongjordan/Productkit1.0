{
  "name": "react_repo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yes | pnpm install && vite",
    "build": "yes | pnpm install && rm -rf node_modules/.vite-temp && tsc -b && vite build",
    "build:prod": "yes | pnpm install && rm -rf node_modules/.vite-temp && tsc -b && BUILD_MODE=prod vite build",
    "build:analyze": "yes | pnpm install && rm -rf node_modules/.vite-temp && tsc -b && BUILD_MODE=prod ANALYZE=true vite build",
    "lint": "yes | pnpm install && eslint .",
    "lint:fix": "yes | pnpm install && eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "yes | pnpm install && vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "vercel-build": "pnpm build:prod"
  },
